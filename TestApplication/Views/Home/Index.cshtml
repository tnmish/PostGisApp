@{
    ViewData["Title"] = "Home Page";
}

<div class="d-flex flex-row-reverse">
    <div class="p-1">
        <div class="row">
            <div class="col col-md-10">
                <button id="addWarehouseBtn" type="button" class="btn btn-primary" onclick="OpenModal()">
                    Добавить склад
                </button>
            </div>
            <div id="hideModal" style="display: none;" class="col col-md-2">
                <button class="close" data-dismiss="modal" area-hidden="true" onclick="CloseModal()">X</button>
            </div>
        </div>
        <div class="row" id="popup" style="display: none;">
            @await Html.PartialAsync("AddWarehouse")
        </div>
    </div>    
    <div class="p-2" id="map"></div>
</div>

<div id="modDialog" class="modal fade">
    <div id="dialogContent" class="modal-dialog"></div>
</div>

<!--секция скриптов-->
@section Scripts {
    <script src="~/js/index.js" asp-append-version="true"></script>
}
